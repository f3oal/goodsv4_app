<h1>Create record</h1>
<br />
<div class="row">
  <div align="center" class="span6 offset3">
    <%= form_for(@good) do |f| %>
     <%= render 'shared/error_messages' %>
      
	    <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :price %>
      <%= f.text_field :price %>
      <br/>
      <%= f.submit "Create", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>